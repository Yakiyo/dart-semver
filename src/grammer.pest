number  = { ASCII_DIGIT+ }
major = { number }
minor = { number }
patch = { number }
prerelease = { number }
prerelease_patch = { number }
channel = { "beta" | "dev" }
extra = { "-" ~ prerelease ~ "." ~ prerelease_patch ~ "." ~ channel }
major_only = { major }
major_minor = { major ~ "." ~ minor }
full_stable = { major ~ "." ~ minor ~ "." ~ patch }
stable = { full_stable | major_minor | major_only }
non_stable = { full_stable ~ extra }
version = { "v"? ~ non_stable | stable }